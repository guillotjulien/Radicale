{
    "name": "radicale-web-v2",
    "version": "1.0.0",
    "description": "A modern yet simple to use web interface for Radicale CalDAV server",
    "scripts": {
        "dev": "gulp",
        "i18n": "gulp i18n",
        "build": "gulp build",
        "format": "prettier --write src/components/**/*.js",
        "lint": "eslint 'src/components/**/*.js'",
        "lint:fix": "eslint --fix 'src/components/**/*.js'"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@clr/ui": "^3.1.0"
    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "eslint": "^6.8.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^6.10.1",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-data": "^1.3.1",
        "gulp-nunjucks-html": "^2.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.0.2",
        "husky": "^4.2.5",
        "lint-staged": "^10.1.5",
        "node-sass": "^4.13.1",
        "prettier": "^2.0.4",
        "uuid": "^7.0.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/components/**/*.js": [
            "eslint",
            "prettier --write"
        ]
    }
}
